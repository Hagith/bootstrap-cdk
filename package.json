{
  "name": "@modernweb/angular-cdk-bootstrap",
  "private": true,
  "scripts": {
    "build": "ng build angular-cdk-bootstrap",
    "build:watch": "ng build angular-cdk-bootstrap --watch",
    "lint": "ng lint angular-cdk-bootstrap",
    "release": "yarn build && cp -a *.md dist/angular-cdk-bootstrap && yarn publish --cwd dist/angular-cdk-bootstrap --access public --non-interactive .",
    "test": "ng test angular-cdk-bootstrap --watch=false",
    "test:watch": "ng test angular-cdk-bootstrap",
    "version:prod": "cd src/ && standard-version -a -i ../CHANGELOG.md",
    "version:rc": "yarn version:prod --prerelease rc",
    "demo": "yarn demo:serve",
    "demo:build": "ng build demo --prod",
    "demo:serve": "ng serve demo --host 0.0.0.0",
    "demo:publish": "yarn demo:build && gh-pages --dist dist/demo"
  },
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^11.0.5",
    "@angular/forms": "^8.2.14",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "rxjs": "~6.5.3",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.19",
    "@angular-devkit/build-ng-packagr": "~0.803.19",
    "@angular/cli": "~8.3.19",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "~10.17.5",
    "bootstrap": "~4.3.1",
    "codelyzer": "^5.2.0",
    "gh-pages": "^2.1.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^5.7.1",
    "protractor": "~5.4.2",
    "standard-version": "^7.0.1",
    "ts-node": "~8.5.2",
    "tsickle": "^0.37.1",
    "tslint": "~5.20.1",
    "typescript": "~3.5.3"
  }
}
