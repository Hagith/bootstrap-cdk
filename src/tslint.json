{
  "extends": "../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "bs",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "bs",
      "kebab-case"
    ],
    "component-class-suffix": false,
    "directive-class-suffix": false,
    "no-host-metadata-property": false
  }
}
